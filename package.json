{
  "name": "cow-swap-visualizer",
  "version": "1.0.0",
  "description": "A TypeScript project to fetch and store CoW Protocol data",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build:frontend": "tsc -b && vite build",
    "start:frontend": "vite",
    "start:backend": "node server.js",
    "sync:historical": "ts-node src/scripts/sync-historical-trades.ts --method blockchain",
    "sync:realtime": "ts-node src/scripts/realtime-settlement-sync.ts"
  },
  "keywords": [
    "cow-protocol",
    "ethereum",
    "defi",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-legacy": "^5.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "mongodb": "^6.19.0",
    "viem": "^2.34.0"
  }
}

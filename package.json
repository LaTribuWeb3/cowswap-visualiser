{
  "name": "cow-swap-visualizer",
  "version": "1.0.0",
  "description": "A TypeScript project to fetch and store CoW Protocol data",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:backend && npm run build:frontend",
    "build:backend": "tsc && npm run copy:config",
    "build:frontend": "vite build",
    "clean": "rimraf dist",
    "copy:config": "node scripts/copy-config.js",
    "dev": "ts-node src/index.ts",
    "dev:frontend": "vite",
    "dev:backend": "ts-node src/index.ts",
    "sync:historical": "ts-node src/scripts/sync-historical-trades.ts",
    "sync:realtime": "ts-node src/scripts/realtime-settlement-sync.ts",
    "db:list": "ts-node src/scripts/list-database-contents.ts",
    "cleanup:database": "ts-node src/scripts/cleanup-database.ts --live",
    "fetch:trades": "ts-node src/scripts/fetch-all-trades.ts",
    "test:trades": "ts-node src/scripts/test-trades-fetch.ts"
  },
  "keywords": [
    "cow-protocol",
    "ethereum",
    "defi",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.23",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-legacy": "^5.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "nodemon": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "sql.js": "^1.11.0",
    "viem": "^2.34.0"
  }
}
